{
  "name": "@segment/action-destinations",
  "description": "Destination Actions engine and definitions.",
  "version": "3.388.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/segmentio/action-destinations",
    "directory": "packages/destination-actions"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": "^18.17 || ^22.13"
  },
  "engineStrict": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "yarn tsc -b tsconfig.build.json",
    "clean": "tsc -b tsconfig.build.json --clean",
    "postclean": "rm -rf dist",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.build.json --noEmit"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.50.0",
    "@bufbuild/protoc-gen-es": "^2.2.3",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^27.0.0",
    "@types/ssh2-sftp-client": "^9.0.0",
    "jest": "^27.3.1",
    "nock": "^13.1.4"
  },
  "dependencies": {
    "@amplitude/ua-parser-js": "^0.7.25",
    "@aws-sdk/client-eventbridge": "^3.741.0",
    "@aws-sdk/client-s3": "^3.600.0",
    "@bufbuild/protobuf": "^2.2.3",
    "@segment/a1-notation": "^2.1.4",
    "@segment/actions-core": "^3.154.0",
    "@segment/actions-shared": "^1.135.0",
    "@types/node": "^22.13.1",
    "ajv-formats": "^2.1.1",
    "aws4": "^1.12.0",
    "cheerio": "^1.0.0-rc.10",
    "dayjs": "^1.10.7",
    "escape-goat": "^3",
    "google-libphonenumber": "^3.2.31",
    "kafkajs": "^2.2.4",
    "liquidjs": "^10.8.4",
    "lodash": "^4.17.21",
    "lru-cache": "10.4.3",
    "ssh2-sftp-client": "^10.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "@segment/ajv-human-errors": "<rootDir>/../ajv-human-errors/src",
      "@segment/actions-core": "<rootDir>/../core/src",
      "@segment/actions-shared": "<rootDir>/../actions-shared/src",
      "@segment/destination-subscriptions": "<rootDir>/../destination-subscriptions/src",
      "@bufbuild/protobuf/codegenv1": "<rootDir>/../../node_modules/@bufbuild/protobuf/dist/cjs/codegenv1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup-after-env.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "__tests__/__helpers__/"
    ]
  },
  "gitHead": "9c04e12c4a0e81f6b68dca0a297fb2c0209d2431"
}
